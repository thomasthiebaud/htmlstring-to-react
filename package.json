{
  "name": "htmlstring-to-react",
  "version": "5.3.0",
  "description": "Convert HTML DOM to React elements",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "build": "npm run clean && tsc --project .",
    "clean": "rimraf lib/ yarn-error.log npm-debug.log",
    "lint": "eslint --ext .ts .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "start": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomasthiebaud/htmlstring-to-react.git"
  },
  "author": "Thomas Thiebaud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomasthiebaud/htmlstring-to-react/issues"
  },
  "homepage": "https://github.com/thomasthiebaud/htmlstring-to-react#readme",
  "files": [
    "lib",
    "types"
  ],
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/dompurify": "2.3.3",
    "@types/enzyme": "3.10.11",
    "@types/jest": "27.4.1",
    "@types/react": "17.0.43",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "conventional-changelog-eslint": "3.0.9",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.2",
    "ts-jest": "27.1.4",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "dompurify": "2.3.6"
  },
  "peerDependencies": {
    "react": "^16.2.0 || ^17.0.0 || ^18.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>tests/setupTests.js"
    ],
    "testEnvironment": "jsdom"
  }
}
